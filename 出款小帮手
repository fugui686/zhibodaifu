    def switch_page(self, index):
        self.stack.setCurrentIndex(index)

    def setup_about_tab(self):
        layout = QVBoxLayout()
        layout.addStretch(1)

        # æ„å»ºå¤–éƒ¨å¡ç‰‡æ¡†
        frame = QFrame()
        frame.setStyleSheet("""
            QFrame {
                border-radius: 15px;  /* è®¾ç½®åœ†è§’è¾¹æ¡†ï¼ŒåŠå¾„ä¸º15åƒç´ ï¼Œä½¿è¾¹è§’åœ†æ¶¦ */
                padding: 25px;        /* å†…è¾¹è·ï¼Œè®©å†…å®¹ä¸è¾¹æ¡†æœ‰è·ç¦»ï¼Œæ›´ç¾è§‚ */
                margin-left: 15px;    /* å·¦è¾¹è·ï¼Œæ§åˆ¶æ•´ä¸ªå¡ç‰‡è·ç¦»å®¹å™¨è¾¹çš„ç©ºéš™ */
                margin-right: 15px;   /* å³è¾¹è·ï¼Œç¡®ä¿æ•´ä½“å±…ä¸­å¯¹ç§° */

