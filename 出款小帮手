    def save_config(self):
        config = configparser.ConfigParser()
        config['SETTINGS'] = {
            'port': self.port_input.text(),
            'cookie': self.cookie_input.text(),
            'code': self.code_input.text()
        }

        with open(CONFIG_FILE, 'w') as configfile:
            config.write(configfile)
        QMessageBox.information(self, "æç¤º", "é…ç½®å·²ä¿å­˜åˆ° config.ini")

    def load_config(self):
        if os.path.exists(CONFIG_FILE):
            config = configparser.ConfigParser()
            config.read(CONFIG_FILE)
            if 'SETTINGS' in config:
                self.port_input.setText(config['SETTINGS'].get('port', ''))
                self.cookie_input.setText(config['SETTINGS'].get('cookie', ''))
                self.code_input.setText(config['SETTINGS'].get('code', ''))

    def fetch_data(self):
        def safe_float(val):
            try:
                return float(val) if val is not None else 0
            except:
                return 0

        def classify_merchant(order_id, raw_name):
            if order_id.startswith('KR'):
                return 'ç¾äººæ¡¥'
            elif order_id.startswith('PF'):
                return 'åå°å……å€¼'

            elif raw_name == 'æ³¢å¸é’±åŒ…':
                return 'æ³¢å¸é’±åŒ…'
            elif not raw_name:
                return 'æœªçŸ¥å•†æˆ·'
            else:
                return 'ç¬¬ä¸‰æ–¹'

        def safe_post(url, data, headers, description=""):
            try:
                resp = requests.post(url, data=data, headers=headers, timeout=60)
                resp.raise_for_status()
                return resp.json()
            except Exception as e:
                QMessageBox.warning(self, "æ¥å£é”™è¯¯", f"{description or url}\n\né”™è¯¯ä¿¡æ¯:\n{str(e)}")
                return None

        # è·å–è¾“å…¥å†…å®¹
        user_id = self.game_id_input.text().strip()
        port = self.port_input.text().strip()
        cookie = self.cookie_input.text().strip()
        code = self.code_input.text().strip()
        start_datetime_str = self.current_time_input.text().strip()
        end_datetime_str = self.last_time_input.text().strip()

        if not all([user_id, port, cookie, code, start_datetime_str, end_datetime_str]):
