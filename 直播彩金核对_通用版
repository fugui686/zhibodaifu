    def set_yesterday(self):
        today = QDate.currentDate()
        yesterday = today.addDays(-1)

        self.start_time_edit.setDateTime(QDateTime(yesterday, QTime(0, 0, 0)))
        self.end_time_edit.setDateTime(QDateTime(today, QTime(0, 0, 0)))

    def show_floating_message(self, message, duration=2000):
        if hasattr(self, '_floating_label') and self._floating_label:
            self._floating_label.deleteLater()

        self._floating_label = QLabel(message, self)
        self._floating_label.setAlignment(Qt.AlignCenter)
        self._floating_label.setStyleSheet("""
            QLabel {
                background-color: #d1f0e1;
                color: #1d5c49;
                font-size: 16px;
                border: 1px solid #a0c5b1;
                border-radius: 8px;
                padding: 10px;
            }
        """)
        self._floating_label.resize(380, 50)

        parent_width = self.width()
        parent_height = self.height()
        self._floating_label.move(
            (parent_width - self._floating_label.width()) // 2,
            (parent_height - self._floating_label.height()) // 2
        )

        self._floating_label.show()

        # Ê≠£Á°ÆÔºöÂÆöÊó∂Âô®ÂõûË∞ÉÊó∂ÂÜçÂÆâÂÖ®Ê∏ÖÈô§
        QTimer.singleShot(duration, self._floating_label.deleteLater)

    def run_script(self):
        self.show_floating_message("ÂêéÂè∞Ê≠£Âú®Ëé∑ÂèñÊï∞ÊçÆÔºåËØ∑Á®çÂÄô...", duration=2000)
        QTimer.singleShot(100, self._run_script_logic)  # Âª∂ËøüÊâßË°å‰∏ªÈÄªËæë
        self.entry_ht.setEnabled(False)
        self.entry_token.setEnabled(False)
        self.entry_port.setEnabled(False)
        self.entry_code.setEnabled(False)
        self.btn_save.setEnabled(False)

        self.start_time_edit.setEnabled(False)
        self.end_time_edit.setEnabled(False)
        self.btn_yesterday.setEnabled(False)
        self.btn_run.setEnabled(False)

    def unlock_ui(self):
        self.entry_ht.setEnabled(True)
        self.entry_token.setEnabled(True)
        self.entry_port.setEnabled(True)
        self.entry_code.setEnabled(True)
        self.btn_save.setEnabled(True)

        self.start_time_edit.setEnabled(True)
        self.end_time_edit.setEnabled(True)
        self.btn_yesterday.setEnabled(True)
        self.btn_run.setEnabled(True)

    def _run_script_logic(self):
        try:
            ht = self.entry_ht.text().strip()
            token = self.entry_token.text().strip()
            port = self.entry_port.text().strip()
            code = self.entry_code.text().strip()

            # ‚úÖ Ê£ÄÊü•ÊòØÂê¶ÊúâÁ©∫ËæìÂÖ•
            if not ht or not token or not port or not code:
                QMessageBox.warning(self, "Ë≠¶Âëä", "‚ùå ÈÖçÁΩÆÈ°π‰∏çËÉΩ‰∏∫Á©∫ÔºåËØ∑Â°´ÂÜôÂÆåÊï¥ÂêéÂÜçËøêË°åÔºÅ")
                self.unlock_ui()
                return

            start_dt = self.start_time_edit.dateTime().toPyDateTime()
            end_dt = self.end_time_edit.dateTime().toPyDateTime()

            # ‚úÖ Êó∂Èó¥ËåÉÂõ¥Ê£ÄÊü•
            if end_dt <= start_dt:
                QMessageBox.warning(self, "Ë≠¶Âëä", "‚ùå ÁªìÊùüÊó∂Èó¥ÂøÖÈ°ªÂ§ß‰∫éÂºÄÂßãÊó∂Èó¥ÔºÅ")
                self.unlock_ui()
                return

            gshzrld = int(start_dt.timestamp())
            gshjrld = int(end_dt.timestamp())

            # Êò®Â§©Êó•ÊúüÊ†ºÂºèÔºàËøîÂà©ÁªìÁÆóÁî®Ôºâ
            start_dt_yesterday = start_dt - timedelta(days=1)
            qtld = start_dt_yesterday.strftime('%Y%m%d')

            headers = {'Cookie': f'token={token}'}
            file_name = f'{ht}ÂΩ©Èáë_{datetime.now().strftime("%YÂπ¥%mÊúà%dÊó•%HÊó∂%MÂàÜ%SÁßí")}.xlsx'

            wb = Workbook()
            ws = wb.active
            ws.title = 'ÂΩ©ÈáëÊ†∏ÂØπ'
            ws.append(['ÂΩíÂ±ûÈÉ®Èó®', '‰ºöÂëòË¥¶Âè∑', 'ÂΩ©ÈáëÂêçÁß∞', 'ÂΩ©ÈáëÈáëÈ¢ù'])

            # Êñ∞Ê¥ªÂä®È¢ÜÂèñ
            url = f"http://{ht}.boinht.com:{port}/campaign/activity_log"
            print(f"Ê≠£Âú®Â§ÑÁêÜÊñ∞Ê¥ªÂä®È¢ÜÂèñ...")
            for page in range(1, 31):
                data = {
                    "page_size": 200, "begin_time": gshzrld, "end_time": gshjrld,
                    "page": page, "type": "0", "login_code": code
                }
                result = safe_post(url, data, headers)
                if not result:
                    break
                append_rows(ws, [[ht, r['platform_user_id'], r['campaign_title'], r['quantity']] for r in result])

            # È¶ñÂ≠òËøîÂà©
            url = f"http://{ht}.boinht.com:{port}/pp_activity_log/pf_list"
            print(f"Ê≠£Âú®Â§ÑÁêÜÊ¥ªÂä®ÊòéÁªÜ[È¶ñÂ≠òËøîÂà©]...")
            for page in range(1, 21):
                data = {
                    "begin_time": gshzrld, "end_time": gshjrld,
                    "page": page, "login_code": code
                }
                result = safe_post(url, data, headers)
                if not result:
                    break
                append_rows(ws, [[ht, r['user_id'], 'È¶ñÂ≠òËøîÂà©', r['money']] for r in result])

            # Áæé‰∫∫Ê°•ÂÖ•Ê¨æ‰ºòÊÉ†
            url = f"http://{ht}.boinht.com:{port}/payment_log/list_payment_log"
            data = {
                'page_size': 99999, 'page': 1, 'is_pay': '1', 'send_item_time': '1',
                'begin_time': gshzrld, 'end_time': gshjrld,
                'is_first_recharge': 0, 'payment_class_id': 999,
                'money_type': 1, "login_code": code
            }
            result = safe_post(url, data, headers)
            print(f"Ê≠£Âú®Â§ÑÁêÜÁæé‰∫∫Ê°•ÂÖ•Ê¨æ‰ºòÊÉ†...")
            for r in result:
                ÈáëÈ¢ù = r.get('money', 0)
                ‰ºòÊÉ† = ÈáëÈ¢ù * 0.02
                ws.append([ht, r['user_id'], 'Áæé‰∫∫Ê°•ÂÖ•Ê¨æ‰ºòÊÉ†', ‰ºòÊÉ†])

            # ÂêéÂè∞ÂÖÖÂÄºÊòéÁªÜ
            data['payment_class_id'] = 777
            result = safe_post(url, data, headers)
            print(f"Ê≠£Âú®Â§ÑÁêÜÂêéÂè∞ÂÖÖÂÄº...")
            print(
                f"ÂêéÂè∞ÂÖÖÂÄºË∑≥Ëøá‰∫ÜËøô‰∫õÂ≠óÊÆµ[ÊîØ‰ªòÂÆùÂõ∫Á†Å„ÄÅÂõ∫Á†ÅÊîØ‰ªòÂÆù„ÄÅÈáç„ÄÅÊ∏Ö„ÄÅ‰∏¢Â§±„ÄÅË∂ÖÊó∂„ÄÅËÆ¢ÂçïÂè∑„ÄÅÊéâÂçï„ÄÅÁõàÂà©ÊèêÂá∫„ÄÅËØØÂ≠ò]ÁöÑÁªüËÆ°")
            for r in result:
                Â§áÊ≥® = r.get('content', '')
                if any(k in Â§áÊ≥® for k in
                       ['ÊîØ‰ªòÂÆùÂõ∫Á†Å', 'Âõ∫Á†ÅÊîØ‰ªòÂÆù', 'Èáç', 'Ê∏Ö', '‰∏¢Â§±', 'Ë∂ÖÊó∂', 'ËÆ¢ÂçïÂè∑', 'ÊéâÂçï', 'ÁõàÂà©ÊèêÂá∫', 'ËØØÂ≠ò']):
                    continue
                ÊÄª = r.get('item_count', 0)
                ÂÆûÈôÖ = r.get('money', 0)
                Ëµ†ÈÄÅ = ÊÄª - ÂÆûÈôÖ
                ws.append([ht, r['user_id'], Â§áÊ≥®, Ëµ†ÈÄÅ])

            # ‚úÖ ËøîÂà©ÁªìÁÆóÔºàËøîÊ∞¥
            rebate_url = f"http://{ht}.boinht.com:{port}/pp_fs_finally_report/get_report_user_list"
            print(f"Ê≠£Âú®Â§ÑÁêÜËøîÂà©ÁªìÁÆó...")
            page = 1
            max_retries = 3  # ÊúÄÂ§öÈáçËØïÊ¨°Êï∞

            while True:
                print(f"Ê≠£Âú®Â§ÑÁêÜËøîÂà©ÁªìÁÆóÁ¨¨ {page} È°µ...")

                success = False
                for attempt in range(1, max_retries + 1):
                    try:
                        data = {
                            "user_ids": "",
                            "report_date": qtld,
                            "page": page,
                            "app_id": "live3",
                            "tree_path": f"/live3/{ht}/",
                            "login_code": code,
                        }

                        response = requests.post(rebate_url, data=data, headers=headers, timeout=10)

                        if response.status_code != 200:
                            print(f"[ËøîÂà©ÁªìÁÆó] Á¨¨ {page} È°µ Á¨¨ {attempt} Ê¨°ËØ∑Ê±ÇÂ§±Ë¥•ÔºåÁä∂ÊÄÅÁ†ÅÔºö{response.status_code}")
                            if attempt < max_retries:
                                print("Á≠âÂæÖ 3 ÁßíÂêéÈáçËØï...")
                                time.sleep(3)
                            continue

                        result = response.json().get('result')
                        if not result:
                            print(f"[ËøîÂà©ÁªìÁÆó] Á¨¨ {page} È°µÊó†Êï∞ÊçÆÔºåÁªìÊùü„ÄÇ")
                            break

                        for r in result:
                            uid = r.get('user_id')
                            Ê∏∏ÊàèËøîÂà© = r.get('game_income', 0)
                            Áõ¥Êí≠ËøîÂà© = r.get('live_income', 0)
                            ÊÄªËøîÂà© = Ê∏∏ÊàèËøîÂà© + Áõ¥Êí≠ËøîÂà©
                            ws.append([ht, uid, 'ËøîÊ∞¥', ÊÄªËøîÂà©])

                        success = True
                        break  # ÂΩìÂâçÈ°µÊàêÂäüÔºåË∑≥Âá∫ÈáçËØïÂæ™ÁéØ

                    except Exception as e:
                        print(f"[ËøîÂà©ÁªìÁÆó] Á¨¨ {page} È°µ Á¨¨ {attempt} Ê¨°ËØ∑Ê±ÇÂºÇÂ∏∏Ôºö{e}")
                        if attempt < max_retries:
                            print("Á≠âÂæÖ 3 ÁßíÂêéÈáçËØï...")
                            time.sleep(3)
                        continue  # ÁªßÁª≠‰∏ã‰∏ÄÊ¨°ÈáçËØï

                if not success:
                    print(f"[ËøîÂà©ÁªìÁÆó] Á¨¨ {page} È°µÈáçËØï {max_retries} Ê¨°ÂêéÂ§±Ë¥•ÔºåÁªàÊ≠¢Âæ™ÁéØ")
                    break

                page += 1

            # ‰øùÂ≠òÂπ∂ÊâìÂºÄÊñá‰ª∂
            wb.save(file_name)
            os.startfile(file_name)

            self.entry_ht.setEnabled(True)
            self.entry_token.setEnabled(True)
            self.entry_port.setEnabled(True)
            self.entry_code.setEnabled(True)
            self.btn_save.setEnabled(True)

            self.start_time_edit.setEnabled(True)
            self.end_time_edit.setEnabled(True)
            self.btn_yesterday.setEnabled(True)
            self.btn_run.setEnabled(True)

        except Exception as e:
            QMessageBox.critical(self, "ÈîôËØØ", str(e))

    def load_config(self):
        config = ConfigParser()
        config.read('config.ini')
        if 'USER' in config:
            return config['USER']['Merchants'], config['USER']['Port'], config['USER']['Token'], config['USER']['Code']
        return '', '', '', ''

    def save_config(self):
        ht = self.entry_ht.text().strip()
        port = self.entry_port.text().strip()
        token = self.entry_token.text().strip()
        code = self.entry_code.text().strip()

        # ‚úÖ Ê£ÄÊü•Á©∫ÂÄº
        if not ht or not port or not token or not code:
            QMessageBox.warning(self, "Ë≠¶Âëä", "‚ùå ÈÖçÁΩÆÈ°π‰∏çËÉΩ‰∏∫Á©∫ÔºåËØ∑Â°´ÂÜôÂÆåÊï¥ÂêéÂÜç‰øùÂ≠òÔºÅ")
            return

        config = ConfigParser()
        config['USER'] = {'Merchants': ht, 'Port': port, 'Token': token, 'Code': code}
        with open('config.ini', 'w') as f:
            config.write(f)

        QMessageBox.information(self, "ÊèêÁ§∫", "‚úÖ ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÔºÅ")


if __name__ == '__main__':
    app = QApplication(sys.argv)

    # ÂºπÂá∫ÂØÜÁ†ÅËæìÂÖ•Ê°Ü
    password, ok = QInputDialog.getText(
        None,
        "ÂØÜÁ†ÅÈ™åËØÅ",
        "ËØ∑ËæìÂÖ•ÂêØÂä®ÂØÜÁ†ÅÔºö",
        QLineEdit.Password
    )

    # Âà§Êñ≠ÂØÜÁ†Å
    if ok and password == "tx007":  # ‰øÆÊîπ‰∏∫‰Ω†Ëá™Â∑±ËÆæÂÆöÁöÑÂØÜÁ†Å
        win = MainWindow()         # Áõ¥Êé•Áî®‰Ω†ÂÆö‰πâÁöÑÁ±ª
        win.show()
        sys.exit(app.exec_())
    else:
        QMessageBox.critical(None, "È™åËØÅÂ§±Ë¥•", "ÂØÜÁ†ÅÈîôËØØÔºåÁ®ãÂ∫èÂç≥Â∞ÜÈÄÄÂá∫„ÄÇ")
        sys.exit(0)


