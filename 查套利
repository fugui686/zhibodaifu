class WorkerSignals(QObject):  # 定义信号类，用于线程间通信
    finished = pyqtSignal(str)


class DataWorker(QRunnable):  # 数据处理线程
    def __init__(self, user_id, signals, port, token, code, start_time, end_time):
        super().__init__()
        self.user_id = user_id
        self.signals = signals
        self.port = port
        self.code = code
        self.token = token
        self.start_time = start_time
        self.end_time = end_time

    def run(self):
        headers = {
            'Cookie': f'token={self.token}',
        }

        # 封装请求函数
        def make_request(url, data):
            for attempt in range(8):
                try:
                    response = requests.post(url, data=data, headers=headers, timeout=10)
                    response.raise_for_status()
                    return response.json().get('result', [])
                except requests.exceptions.HTTPError as http_err:
                    if response.status_code == 502:
                        print(f"请求失败 502, 正在重试... 第 {attempt + 1} 次")
                        time.sleep(3)
                    else:
                        print(f"请求失败: {http_err} 接口: {url}")
                        return []
                except Exception as e:
                    print(f"其他错误: {str(e)} 接口: {url}")
                    return []
                time.sleep(1)
            return []

        url1 = f"http://xingfu.boinht.com:{self.port}/user/pf_list_with_game"
        data1 = {
            "channel_id": "",
            "open_channel_id": "",
            "id": "",
            "game_user_id": self.user_id,
            "is_bind": "",
            "is_black": "",
            "nickname": "",
            "is_online": "",
            "reg_ip": "",
            "is_recharge": "",
            "device_type": "",
            "mobile": "",
            "apns_code": "",
            "begin_time": '',
            "end_time": '',
            "page_size": 32,
            "page": 1,
            "login_code": self.code,
        }

        data11 = make_request(url1, data1)
        输出结果 = f"{self.user_id}"

        for aa in data11:
            游戏ID = aa.get('game_user_id', '无数据')
            APPID = aa.get('app_user_id', '无数据')
            注册时间 = datetime.fromtimestamp(int(aa.get('create_time', 0)))
            最后登录时间 = datetime.fromtimestamp(int(aa.get('last_login', 0)))
            注册 = aa.get('reg_ip', '')
            注册ip地区 = aa.get('reg_ip_adress', '')
            最后登录 = aa.get('last_ip', '')
            最后登录ip地区 = aa.get('pre_ip_adress', '')
            商户 = aa.get('channel_id')
            渠道 = aa.get('open_channel_id')
            打赏 = aa.get('use_diamond', 0)
            钻石 = aa.get('diamond', 0)
            总充值 = aa.get('total_recharge', 0)
            总提现 = aa.get('total_cash', 0)
            余额 = aa.get('money', 0)

            注册IP = f'{注册} / {注册ip地区}'
            最后登录IP = f'{最后登录} / {最后登录ip地区}'

            url3 = f"http://xingfu.boinht.com:{self.port}/pp_user/recent_overview_by_userid"
            data3 = {
                "user_id": 游戏ID,
                "start_time": self.start_time,
                "end_time": self.end_time,
                "login_code": self.code,
            }

            data33 = make_request(url3, data3)

            for cc in data33:
                有效投注 = cc.get('vaild_bet', 0) or 0
                游戏损益 = cc.get('delta_bet_win', 0) or 0
                充值 = cc.get('recharge', 0) or 0
                提现 = cc.get('cash', 0) or 0
                提充客损 = cc.get('delta_recharge_cash', 0) or 0

                # 商户列表
                url商户 = f"http://xingfu.boinht.com:{self.port}/payment_log/list_report_payment_log"
                data商户 = {
                    "page_size": 99999,
                    "page": 1,
                    "begin_date": datetime.fromtimestamp(self.start_time).strftime('%Y%m%d00'),
                    "end_date": datetime.fromtimestamp(self.end_time).strftime('%Y%m%d00'),
                    "sort_field": "money",
                    "sort_type": "desc",
                    "is_effect": 0,
                    "login_code": self.code,
                }
                商户数据 = make_request(url商户, data商户)
                商户字典 = {str(item.get("payment_class_id")): item.get("firm_name") for item in 商户数据}

                # 充值明细
                url明细 = f"http://xingfu.boinht.com:{self.port}/payment_log/list_payment_log"
                data明细 = {
                    "page_size": 99999,
                    "page": 1,
                    "begin_time": self.start_time,
                    "end_time": self.end_time,
                    "is_pay": "1",
                    "money_type": 1,
                    "login_code": self.code,
                    "user_id": APPID,
                }
                明细数据 = make_request(url明细, data明细)

                充值方式列表 = []
                备注列表 = []

                for 明细 in 明细数据:
                    商户ID = 明细.get('payment_class_id')
                    内部订单号 = 明细.get('order_id', '')
                    当前备注 = str(明细.get('content', '')).strip()

                    商户名称 = 商户字典.get(str(商户ID))
                    if not 商户名称:
                        if 内部订单号.startswith('KR'):
                            商户名称 = '人工充值'
                        elif 内部订单号.startswith('PF'):
                            商户名称 = '后台充值'
                        else:
                            商户名称 = '未知商户'

                    # ➕ 充值方式加进列表（不管是不是后台）
                    if 商户名称:
                        充值方式列表.append(商户名称)

                    # ➕ 只有后台充值才加备注
                    if 商户名称 == '后台充值':
                        备注列表.append(当前备注 if 当前备注 else '无备注')

                # ✅ 充值方式去重 + 保留顺序
                充值方式 = "\n ".join(dict.fromkeys(充值方式列表)) if 充值方式列表 else "无充值方式"

                # ✅ 备注去重 + 保留顺序
                备注 = "\n ".join(dict.fromkeys(备注列表)) if 备注列表 else "无备注"

                # 设备号合集
                url设备 = f"http://xingfu.boinht.com:{self.port}/user/list_user_login_divice"
                data设备 = {"user_id": APPID, "login_code": self.code}
                设备数据 = make_request(url设备, data设备)

                设备号合集列表 = [item.get('apnsCode') for item in 设备数据 if item.get('apnsCode')]
                设备号合集 = "\n ".join(设备号合集列表) if 设备号合集列表 else "无设备"

                # 状态
                url状态 = f"http://xingfu.boinht.com:{self.port}/user/get_user_game_data_by_app_userid"
                data状态 = {"user_id": APPID, "login_code": self.code}
                状态数据 = make_request(url状态, data状态)
                状态值 = 状态数据[0].get('is_game_black', 0) if 状态数据 else 0
                状态 = '正常' if 状态值 == 0 else '已冻结'

                # 打码倍数
                打码倍数 = round(有效投注 / 充值, 2) if 充值 else 0

                # 拼接输出
                输出结果 += f", {APPID}, {渠道}, {注册时间}, {注册IP}, {最后登录IP}, {充值}, {充值方式}, {备注}, {提现}, {提充客损}, {打码倍数}, {设备号合集}, {状态}"

        print(输出结果)
        self.signals.finished.emit(输出结果)


class UserWindow(QWidget):  # 主窗口类
    def __init__(self):
        super().__init__()

        self.setWindowTitle("游戏ID查套利")

        # 加载 base64 图标（确保字符串完整）
        icon_base64 = ""  # 替换成你的 base64
        icon_data = QByteArray.fromBase64(icon_base64.encode())

        # 更可靠的图标加载方式
        pixmap = QPixmap()
        if not pixmap.loadFromData(icon_data, "ICO"):  # 显式指定格式
            print("❌ 图标加载失败！请检查 base64 数据或文件格式")
        else:
            self.setWindowIcon(QIcon(pixmap))  # 设置窗口和任务栏图标

        self.resize(1000, 650)  # 初始大小
        self.setMinimumSize(800, 500)  # 限制最小

        self.groups = ["绿茶", "妖姬", "色戒", "爱色", "性福", "少女", '人妻', '乐咪']
        self.token_inputs = {}
        self.thread_pool = QThreadPool()
        self.completed_tasks = []  # 初始化已完成任务列表

        # 设置主窗口样式
        self.setStyleSheet("""
            QPushButton {
                background-color: white;            /* 白色背景 */
                color: #3498db;                     /* 蓝色文字 */
                border: 1px solid #3498db;          /* 蓝色边框 */
                border-radius: 8px;                 /* 圆角半径8px */
                padding: 10px 16px;                 /* 内边距 */
                font-size: 16px;                    /* 字体大小 */
                font-weight: bold;                 /* 字体加粗 */
                letter-spacing: 0.5px;              /* 字符间距 */
            }

            QPushButton:hover {
                background-color: #ecf5fb;          /* 鼠标悬停时浅蓝背景（视觉反馈） */
                color: #2980b9;                     /* 悬停时文字颜色稍微加深 */
                border: 1px solid #2980b9;          /* 边框颜色也稍微加深 */
            }

            QPushButton:pressed {
                background-color: #d6eaf8;          /* 按下时更深一点的浅蓝背景 */
                padding-left: 14px;                 /* 模拟按压感 */
                padding-top: 12px;
            }

            QPushButton:disabled {
                background-color: #f0f0f0;          /* 禁用时背景灰色 */
                color: #bdc3c7;                     /* 禁用时字体浅灰 */
                border: 1px solid #ccc;             /* 边框变浅灰 */
            }

            QLineEdit, QComboBox, QDateTimeEdit {
                padding: 6px;                      /* 输入控件内边距（6px） */
                border: 1px solid #ccc;            /* 边框颜色为浅灰色 */
                border-radius: 5px;                /* 圆角（5px） */
                font-size: 20px;                   /* 字体大小（20px） */
            }

            QGroupBox {
                background-color: #f9f9f9;         /* 分组框背景颜色（浅灰白） */
                border: 1px solid #dcdcdc;         /* 边框颜色（亮灰） */
                border-radius: 8px;                /* 分组框圆角半径（8px） */
                margin-top: 20px;                  /* 与上方控件间距（20px） */
                padding: 15px;                     /* 内边距（15px，增强呼吸感） */
            }

            QGroupBox::title {
                subcontrol-origin: margin;         /* 标题位置从外边距起算 */
                subcontrol-position: top left;     /* 标题靠近左上角对齐 */
                padding: 0 8px;                    /* 标题左右内边距（8px） */
                background-color: #f9f9f9;         /* 标题背景色与框体一致 */
                color: #2c3e50;                    /* 标题文字颜色（深蓝灰） */
                font-weight: bold;                 /* 标题加粗，提升辨识度 */
            }
        """)

        main_layout = QVBoxLayout(self)  # 设置主布局为垂直布局

        # 内容区域：导航栏 + 页面
        content_layout = QHBoxLayout()  # 水平布局容纳导航栏和页面

        # 左侧导航栏
        self.nav_list = QListWidget()
        self.nav_list.setFixedWidth(200)  # 设置固定宽度
        self.nav_list.setFont(QFont("Microsoft YaHei", 11))  # 设置字体和大小
        # 设置导航栏样式
        self.nav_list.setStyleSheet("""
                    QListWidget {
                        background-color: #2c3e50;  /* 列表控件背景颜色（深蓝色） */
                        color: #ecf0f1;            /* 列表默认文字颜色（浅灰色） */
                        border: none;              /* 无边框 */
                        padding-top: 15px;        /* 顶部内边距（15px） */
                        outline: none;            /* 移除焦点时的虚线边框 */
                    }
                    QListWidget::item {
                        padding: 12px 20px;       /* 列表项内边距（上下12px，左右20px） */
                        margin: 6px 10px;         /* 列表项外边距（上下6px，左右10px） */
                        font-weight: normal;      /* 正常字体粗细 */
                        border-radius: 6px;       /* 列表项圆角半径（6px） */
                    }
                    QListWidget::item:hover {
                        background-color: #3d566e;  /* 鼠标悬停时背景颜色（稍亮的蓝色） */
                        font-weight: bold;         /* 悬停时字体加粗 */
                        color: #ffffff;            /* 悬停时文字颜色（纯白） */
                    }
                    QListWidget::item:selected {
                        background-color: #1abc9c;  /* 选中项背景颜色（蓝绿色） */
                        color: #ffffff;            /* 选中项文字颜色（纯白） */
                        font-weight: bold;         /* 选中项字体加粗 */
                    }
                """)

        item1 = QListWidgetItem("📊 数据获取")
        item2 = QListWidgetItem("⚙️ 参数配置")
        item3 = QListWidgetItem("📘 关于我们")

        item1.setSizeHint(QSize(item1.sizeHint().width(), 40))
        item2.setSizeHint(QSize(item2.sizeHint().width(), 40))
        item3.setSizeHint(QSize(item3.sizeHint().width(), 40))

        self.nav_list.addItem(item1)
        self.nav_list.addItem(item2)
        self.nav_list.addItem(item3)

        self.nav_list.setCurrentRow(0)
        self.nav_list.currentRowChanged.connect(self.switch_page)

        # 右侧页面栈
        self.pages = QStackedWidget()
        self.data_tab = QWidget()
        self.config_tab = QWidget()
        self.about_tab = QWidget()
        self.pages.addWidget(self.data_tab)
        self.pages.addWidget(self.config_tab)
        self.pages.addWidget(self.about_tab)

        content_layout.addWidget(self.nav_list)
        content_layout.addWidget(self.pages)
        main_layout.addLayout(content_layout)

        # 状态栏与进度条
        self.status_label = QLabel("状态：等待中")
        self.progress_bar = QProgressBar()
        self.progress_bar.setVisible(False)
        main_layout.addWidget(self.status_label)
        main_layout.addWidget(self.progress_bar)

        # 页面设置
        self.setup_data_tab()
        self.setup_config_tab()
        self.setup_about_tab()

        self.load_config()
        self.set_default_dates()

    def switch_page(self, index):
        self.pages.setCurrentIndex(index)

    def setup_data_tab(self):  # 数据获取页面
        layout = QVBoxLayout()

        # 文件与线程设置
        file_group = QGroupBox("分组选择")
        file_layout = QVBoxLayout()

        self.file_button = QPushButton("📁 选择数据文件")
        self.file_button.clicked.connect(self.select_file)
        file_layout.addWidget(self.file_button)

        self.thread_count_input = QLineEdit()
        self.thread_count_input.setPlaceholderText("多开台数（1-100）")
        file_layout.addWidget(self.thread_count_input)

        self.group_select_home = QComboBox()
        self.group_select_home.addItems(self.groups)
        file_layout.addWidget(self.group_select_home)

        file_group.setLayout(file_layout)
        layout.addWidget(file_group)

        # 日期范围
        date_group = QGroupBox("日期范围选择")
        date_layout = QVBoxLayout()

        self.start_time_input = QDateTimeEdit()
        self.start_time_input.setCalendarPopup(True)
        self.start_time_input.setDisplayFormat("yyyy-MM-dd HH:mm:ss")

        self.end_time_input = QDateTimeEdit()
        self.end_time_input.setCalendarPopup(True)
        self.end_time_input.setDisplayFormat("yyyy-MM-dd HH:mm:ss")

        date_layout.addWidget(QLabel("开始时间："))
        date_layout.addWidget(self.start_time_input)
        date_layout.addWidget(QLabel("结束时间："))
        date_layout.addWidget(self.end_time_input)

        nav_layout = QHBoxLayout()

        self.prev_day_btn = QPushButton("⏪ 上一日")
        self.prev_day_btn.clicked.connect(self.on_prev_day_clicked)
        nav_layout.addWidget(self.prev_day_btn)

        self.today_btn = QPushButton("⏺️ 当日")
        self.today_btn.clicked.connect(self.on_today_clicked)
        nav_layout.addWidget(self.today_btn)

        self.next_day_btn = QPushButton("下一日 ⏩")
        self.next_day_btn.clicked.connect(self.on_next_day_clicked)
        nav_layout.addWidget(self.next_day_btn)

        date_layout.addLayout(nav_layout)
        date_group.setLayout(date_layout)
        layout.addWidget(date_group)

        # 操作按钮水平布局
        button_layout = QHBoxLayout()

        self.get_button = QPushButton("🚀 开始获取")
        self.get_button.clicked.connect(self.start_processing)
        button_layout.addWidget(self.get_button)

        self.export_button = QPushButton("📤 导出数据")
        self.export_button.clicked.connect(self.export_data)
        button_layout.addWidget(self.export_button)

        layout.addLayout(button_layout)  # 将按钮水平布局添加到主布局中

        self.data_tab.setLayout(layout)

        # 初始样式设置
        self.highlight_selected_button(self.today_btn)

    def highlight_selected_button(self, selected_button):  # 高亮选中按钮
        buttons = [self.prev_day_btn, self.today_btn, self.next_day_btn]
        for btn in buttons:
            if btn == selected_button:
                btn.setStyleSheet("""
                    QPushButton {
                        background-color: #1DA1F2;
                        color: white;
                        border-radius: 6px;
                        padding: 8px 14px;
                        font-weight: bold;
                    }
                    QPushButton:hover {
                        background-color: #0d8ddf;
                    }
                """)
            else:
                btn.setStyleSheet("""
                    QPushButton {
                        background-color: white;
                        color: #3498db;
                        border: 1px solid #3498db;
                        border-radius: 6px;
                        padding: 8px 14px;
                        font-weight: bold;
                    }
                    QPushButton:hover {
                        background-color: #ecf5fb;
                        color: #2980b9;
                        border: 1px solid #2980b9;
                    }
                """)

    # 封装点击事件方法
    def on_prev_day_clicked(self):
        self.move_to_previous_day()
        self.highlight_selected_button(self.prev_day_btn)

    def on_today_clicked(self):
        self.set_default_dates()
        self.highlight_selected_button(self.today_btn)

    def on_next_day_clicked(self):
        self.move_to_next_day()
        self.highlight_selected_button(self.next_day_btn)

    def setup_config_tab(self):  # 配置页面
        layout = QVBoxLayout()

        config_group = QGroupBox("分组 Cookie 配置")
        grid = QGridLayout()

        for i, group in enumerate(self.groups):
            label = QLabel(f"{group} Cookie：")
            line_edit = QLineEdit()
            line_edit.setPlaceholderText("请输入平台返回的 Cookie 值")
            self.token_inputs[group] = line_edit
            grid.addWidget(label, i, 0)
            grid.addWidget(line_edit, i, 1)

        config_group.setLayout(grid)
        layout.addWidget(config_group)

        # 端口配置
        port_group = QGroupBox("基础参数")
        port_layout = QVBoxLayout()

        # 端口输入行（横向布局）
        port_row = QHBoxLayout()
        port_label = QLabel("端口号：")
        self.port_input = QLineEdit()
        self.port_input.setPlaceholderText("请输入端口号，例如：8080")
        port_row.addWidget(port_label)
        port_row.addWidget(self.port_input)
        port_layout.addLayout(port_row)

        # 登录码输入行（横向布局）
        code_row = QHBoxLayout()
        code_label = QLabel("小明登录码：")
        self.code_input = QLineEdit()
        self.code_input.setPlaceholderText("请输入小明计算器登陆码")
        code_row.addWidget(code_label)
        code_row.addWidget(self.code_input)
        port_layout.addLayout(code_row)

        port_group.setLayout(port_layout)
        layout.addWidget(port_group)

        self.save_button = QPushButton("💾 保存配置")
        self.save_button.clicked.connect(self.manual_save)
        layout.addWidget(self.save_button)

        self.config_tab.setLayout(layout)

    def setup_about_tab(self):  # 关于我们页面
        layout = QVBoxLayout()
        layout.addStretch(1)

        # 构建外部卡片框
        frame = QFrame()
        frame.setStyleSheet("""
            QFrame {
                border-radius: 15px;  /* 设置圆角边框，半径为15像素，使边角圆润 */
                padding: 25px;        /* 内边距，让内容与边框有距离，更美观 */
                margin-left: 15px;    /* 左边距，控制整个卡片距离容器边的空隙 */
                margin-right: 15px;   /* 右边距，确保整体居中对称 */

                background: qlineargradient(  /* 设置线性渐变背景 */
                    spread:pad,               /* 渐变填充模式：pad 表示边缘以最后一个颜色填充 */
                    x1:0, y1:0,               /* 渐变起点：左上角 */
                    x2:1, y2:1,               /* 渐变终点：右下角，形成对角渐变 */
                    stop:0 #e0f7fa,           /* 渐变起始颜色：浅蓝色 */
                    stop:1 #d1c4e9            /* 渐变结束颜色：浅紫色 */
                );

                border: 1px solid #b0bec5;    /* 设置边框为1像素实线，颜色为灰蓝色，增强边界清晰度 */
            }
        """)
        # 添加阴影效果
        shadow = QGraphicsDropShadowEffect()
        shadow.setBlurRadius(25)
        shadow.setXOffset(0)
        shadow.setYOffset(4)
        shadow.setColor(QColor(0, 0, 0, 80))
        frame.setGraphicsEffect(shadow)

        frame_layout = QVBoxLayout()

        # ===== Base64 logo 处理 =====
        logo_base64 = b"""
        
        """  # 你可以替换为你自己的 base64 字符串（确保是 PNG 或 JPG）

        logo_label = QLabel()
        image_data = base64.b64decode(logo_base64)
        pixmap = QPixmap()
        pixmap.loadFromData(image_data)

        logo_label.setPixmap(pixmap.scaled(80, 80, Qt.KeepAspectRatio, Qt.SmoothTransformation))
        logo_label.setAlignment(Qt.AlignCenter)
        frame_layout.addWidget(logo_label)

        # 文本内容
        about_text = QLabel("""
            <h2 style="text-align:center; color:#2c3e50;">关于 小燕子</h2>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                <b>小燕子</b> 是基于 Python 开发的轻量级 <b style="color:#e67e22;">网页爬虫工具</b>，用于结构化抓取动态网站数据。
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                支持 <b>Cookie 登录态管理</b>、<b>多线程任务并发</b>、<b>定时调度执行</b> 和 <b>按分组配置抓取策略</b>。
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                可用于接口调用分析、数据自动备份、评论追踪、内容监控等各类数据采集任务。
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                提供可视化操作界面，降低配置门槛，适合数据工程、运维、分析等角色快速部署与使用。
            </p>
            <hr style="border: none; border-top: 1px solid #ccc; margin: 12px 60px;" />
            </p>
            <p style="text-align:center; font-size:14px; color:#34495e;">
                版本号：<b style="color:#27ae60;">2025.07.06</b>
            </p>
        """)

        about_text.setAlignment(Qt.AlignHCenter)
        about_text.setTextFormat(Qt.RichText)
        about_text.setWordWrap(True)

        frame_layout.addWidget(about_text)
        frame.setLayout(frame_layout)

        layout.addWidget(frame)
        layout.addStretch(1)
        self.about_tab.setLayout(layout)

    def set_default_dates(self):  # 设置默认日期为今天和明天
        current_date = datetime.now().date()
        tomorrow = current_date + timedelta(days=1)
        self.start_time_input.setDateTime(QDateTime(current_date.year, current_date.month, current_date.day, 0, 0))
        self.end_time_input.setDateTime(QDateTime(tomorrow.year, tomorrow.month, tomorrow.day, 0, 0))

    def move_to_previous_day(self):  # 移动到前一天
        date = self.start_time_input.dateTime().addDays(-1)
        self.start_time_input.setDateTime(date)
        self.end_time_input.setDateTime(date.addDays(1))

    def move_to_next_day(self):  # 移动到后一天
        date = self.start_time_input.dateTime().addDays(1)
        self.start_time_input.setDateTime(date)
        self.end_time_input.setDateTime(date.addDays(1))

    def select_file(self):  # 选择用户ID文件
        desktop_path = QStandardPaths.writableLocation(QStandardPaths.DesktopLocation)  # 获取桌面路径
        file_name, _ = QFileDialog.getOpenFileName(self, "选择用户ID文件", desktop_path,
                                                   "Text Files (*.txt);;All Files (*)")
        if file_name:
            error_lines = []  # 用于记录包含非数字字符的行号
            self.user_ids = []  # 清空之前的用户ID列表

            try:
                with open(file_name, 'r', encoding='utf-8') as file:  # 使用utf-8编码打开文件
                    for line_number, line in enumerate(file, start=1):  # 逐行读取文件
                        line = line.strip()  # 去除空白字符
                        if line:  # 如果行不为空
                            if line.isdigit():  # 检查是否为纯数字
                                self.user_ids.append(line)  # 如果是数字，添加到用户ID列表
                            else:
                                error_lines.append(line_number)  # 记录错误行号

                if error_lines:
                    # 如果有错误行，提示用户
                    error_message = f"文件中有非数字字符，错误行号：{', '.join(map(str, error_lines))}"
                    QMessageBox.warning(self, "错误", error_message)
                else:
                    # 如果没有错误，提示加载成功
                    QMessageBox.information(self, "提示", f"已加载 {len(self.user_ids)} 个用户ID！")
                    self.get_button.setEnabled(True)

            except UnicodeDecodeError:
                # 如果文件编码错误，提示用户
                QMessageBox.warning(self, "错误", "文件编码错误，请确保文件使用UTF-8编码！")

    def start_processing(self):  # 开始处理数据
        # 检查是否选择了文件
        if not hasattr(self, 'user_ids') or not self.user_ids:
            QMessageBox.warning(self, "提示", "请先选择包含用户ID的文件！")
            return

        # 设置允许使用的截止时间 (YYYY-MM-DD HH:MM:SS)
        self.expiration_date_str = "2025-12-06 23:59:59"  # 软件过期时间
        self.expiration_date = datetime.strptime(self.expiration_date_str, "%Y-%m-%d %H:%M:%S")

        # 检查是否需要提醒
        remaining_days = self.get_remaining_days()
        if remaining_days > 0 and remaining_days <= 7:
            QMessageBox.warning(self, "软件即将更新",
                                f"本软件将在{remaining_days}天后自动更新：\n请联系《富贵》获取最新版本。")

        # 检查是否过期
        if self.is_expired():
            QMessageBox.critical(self, "软件已更新", f"你的应用版本过低：\n请联系《富贵》获取最新版本。")
            sys.exit(0)  # 退出程序

        self.completed_tasks = []  # 用于存储已完成的任务
        port = self.port_input.text().strip()
        group = self.group_select_home.currentText()
        token = self.token_inputs[group].text().strip()
        code = self.code_input.text().strip()

        missing_fields = []
        if not port:
            missing_fields.append("端口（port）")
        if not code:
            missing_fields.append("验证码（code）")
        if not token:
            missing_fields.append(f"小组 {group} 的 Cookie")

        if missing_fields:
            QMessageBox.warning(self, "输入错误", f"请填写以下内容：\n" + "\n".join(missing_fields))
            return

        # 检查时间输入
        start = self.start_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        end = self.end_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        if end < start:
            QMessageBox.warning(self, '警告', '结束时间不能早于开始时间')
            return

        # 将字符串转换为datetime对象
        start_datetime = datetime.strptime(start, '%Y-%m-%d %H:%M:%S')
        end_datetime = datetime.strptime(end, '%Y-%m-%d %H:%M:%S')
        print(f"开始时间: {start_datetime}, 结束时间: {end_datetime}")

        # 将时间转换为时间戳
        start_time = int(start_datetime.timestamp())
        end_time = int(end_datetime.timestamp())

        # 获取用户输入的线程数量
        thread_count = self.thread_count_input.text()
        if thread_count.isdigit() and int(thread_count) > 0:
            max_threads = min(int(thread_count), 100)  # 限制线程数量最高为100
            self.thread_pool.setMaxThreadCount(max_threads)
        else:
            QMessageBox.warning(self, "输入错误", "请确保多开数量为正整数！")
            return

        # 更新状态栏
        self.status_label.setText(f"状态：正在获取数据 (0/{len(self.user_ids)})")
        self.progress_bar.setMaximum(len(self.user_ids))
        self.progress_bar.setValue(0)
        self.progress_bar.setVisible(True)

        for user_id in self.user_ids:
            signals = WorkerSignals()
            signals.finished.connect(self.task_finished)
            worker = DataWorker(user_id, signals, port, token, code, start_time, end_time)
            self.thread_pool.start(worker)

            self.file_button.setEnabled(False)  # 禁用选择文件按钮
            self.thread_count_input.setEnabled(False)  # 禁用线程输入
            self.group_select_home.setEnabled(False)  # 禁用小组选择选择
            self.start_time_input.setEnabled(False)  # 禁用开始时间
            self.end_time_input.setEnabled(False)  # 禁用结束按钮
            self.get_button.setEnabled(False)  # 禁用获取按钮
            self.export_button.setEnabled(False)  # 禁用导出按钮
            self.prev_day_btn.setEnabled(False)  # 禁用上一日按钮
            self.today_btn.setEnabled(False)  # 禁用当日按钮
            self.next_day_btn.setEnabled(False)  # 禁用下一日按钮

    def is_expired(self):
        """检查当前时间是否超过指定的截止时间"""
        current_time = datetime.now()  # 获取当前系统时间
        return current_time > self.expiration_date

    def get_remaining_days(self):
        """计算距离过期时间还有多少天"""
        current_time = datetime.now()  # 获取当前系统时间
        days_left = (self.expiration_date - current_time).days  # 计算剩余天数
        return days_left  # 返回剩余的天数

    def task_finished(self, result):  # 任务完成的回调函数
        self.completed_tasks.append(result)
        # 更新进度条和状态
        progress = len(self.completed_tasks)
        total = len(self.user_ids)
        self.progress_bar.setValue(progress)
        self.status_label.setText(f"状态：正在获取数据 ({progress}/{total})")

        if progress == total:
            self.status_label.setText("状态：所有数据已获取完成！")
            self.progress_bar.setVisible(False)

        if len(self.completed_tasks) == len(self.user_ids):
            QMessageBox.information(self, "提示", f"所有数据已获取完成！")
            self.file_button.setEnabled(True)  # 启用选择文件按钮
            self.thread_count_input.setEnabled(True)  # 启用线程输入
            self.group_select_home.setEnabled(True)  # 启用小组选择选择
            self.start_time_input.setEnabled(True)  # 启用开始时间
            self.end_time_input.setEnabled(True)  # 启用结束按钮
            self.get_button.setEnabled(True)  # 启用获取按钮
            self.export_button.setEnabled(True)  # 启用导出按钮
            self.prev_day_btn.setEnabled(True)  # 启用上一日按钮
            self.today_btn.setEnabled(True)  # 启用当日按钮
            self.next_day_btn.setEnabled(True)  # 启用下一日按钮

    def export_data(self):  # 导出数据到Excel文件
        if not self.completed_tasks:
            QMessageBox.warning(self, "警告", "没有数据可导出！")
            return

        try:
            wb = Workbook()
            ws = wb.active
            ws.append(['渠道', 'APPID', '游戏ID', '注册时间', '注册IP', '最后登录IP', '充值', '充值方式',
                       '彩金名称', '提现', '提充客损', '打码倍数', '设备号合集', '投注游戏', '状态'])  # 更新表头

            for task in self.completed_tasks:
                parts = task.split(", ")
                # 使用默认值处理可能缺失的数据
                游戏ID = self.convert_to_number(parts[0]) if len(parts) > 0 else "无数据"
                APPID = self.convert_to_number(parts[1]) if len(parts) > 1 else "无数据"
                渠道号 = parts[2] if len(parts) > 2 else "无数据"  # 合并的渠道信息
                注册时间 = parts[3] if len(parts) > 3 else "无数据"
                注册IP = parts[4] if len(parts) > 4 else "无数据"
                最后登录IP = parts[5] if len(parts) > 5 else "无数据"
                充值 = self.convert_to_number(parts[6]) if len(parts) > 6 else "无数据"
                充值方式 = parts[7] if len(parts) > 7 else "无数据"
                备注 = parts[8] if len(parts) > 8 else "无数据"
                提现 = self.convert_to_number(parts[9]) if len(parts) > 9 else "无数据"
                提充客损 = self.convert_to_number(parts[10]) if len(parts) > 10 else "无数据"
                打码倍数 = self.convert_to_number(parts[11]) if len(parts) > 11 else "无数据"
                设备号合集 = parts[12] if len(parts) > 12 else "无数据"
                状态 = parts[13] if len(parts) > 13 else "无数据"

                # 更新数据行
                ws.append([渠道号, APPID, 游戏ID, 注册时间, 注册IP, 最后登录IP, 充值, 充值方式, 备注, 提现, 提充客损, 打码倍数, 设备号合集, '', 状态])

            current_time = datetime.now().strftime("%Y年%m月%d日%H时%M分%S秒")
            filename = f"导出数据_{current_time}.xlsx"
            wb.save(filename)
            QMessageBox.information(self, "提示", f"数据已导出到 {filename}")
            os.startfile(filename)

        except Exception as e:
            QMessageBox.critical(self, "导出错误", f"导出数据时发生错误: {str(e)}")

    def convert_to_number(self, value):  # 将字符串转换为数字，如果转换失败则返回"无数据"
        try:
            return float(value)  # 尝试将值转换为浮点数
        except ValueError:
            return "无数据"  # 转换失败时返回"无数据"

    def save_config(self, port, token_dict, code):  # 保存配置文件
        config = ConfigParser()

        # 不再读取旧配置，直接清空并写入新的
        config['COMMON'] = {'Port': port, 'Code': code}
        for group, token in token_dict.items():
            config[group] = {'Token': token}

        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):  # 加载配置文件
        config = ConfigParser()
        config.read('config.ini')

        if 'COMMON' in config:
            self.port_input.setText(config['COMMON'].get('Port', ''))
            self.code_input.setText(config['COMMON'].get('Code', ''))

        for group in self.groups:
            if group in config:
                self.token_inputs[group].setText(config[group].get('Token', ''))

    def manual_save(self):  # 手动保存配置
        port = self.port_input.text()
        code = self.code_input.text()

        token_dict = {}
        missing_groups = []

        for group, input_field in self.token_inputs.items():
            token = input_field.text()
            if token:
                token_dict[group] = token
                # 设置为正常状态（清除红色边框）
                input_field.setStyleSheet("")
            else:
                missing_groups.append(group)
                # 设置红色边框
                input_field.setStyleSheet("border: 1px solid red;")

        if not port or not code:
            QMessageBox.warning(self, "警告", "请填写完整的配置（port和code）！")
            return

        self.save_config(port, token_dict, code)

        if missing_groups:
            missing_text = "、".join(missing_groups)
            QMessageBox.information(self, "部分保存成功", f"配置已保存，但以下小组未填写 Cookie：{missing_text}")
        else:
            QMessageBox.information(self, "提示", "配置已完整保存！")


if __name__ == "__main__":
    app = QApplication(sys.argv)

    # 弹出密码输入框
    password, ok = QInputDialog.getText(
        None,
        "密码验证",
        "请输入启动密码：",
        QLineEdit.Password
    )

    # 判断密码
    if ok and password == "tx007":  # 修改为你自己设定的密码
        window = UserWindow()         # 直接用你定义的类
        window.show()
        sys.exit(app.exec_())
    else:
        QMessageBox.critical(None, "验证失败", "密码错误，程序即将退出。")
        sys.exit(0)
